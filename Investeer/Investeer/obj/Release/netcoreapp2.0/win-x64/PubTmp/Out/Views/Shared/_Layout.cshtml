    <script type="text/javascript">
        $(document).ready(function () {
            $('#preloader').hide();
            $(document).ajaxStart(function () {
                $('#preloader').show();
            }).ajaxStop(function () {
                $('#preloader').hide();
            });
        });
        const toast = (type, msg) => {
            if (type == "success") {
                toastr.success(msg);
            }
            else if (type == "info") {
                toastr.info(msg);
            }
            else if (type == "error") {
                toastr.error(msg);
            }
            else if (type == "warning") {
                toastr.warning(msg);
            }
        }
       
    </script>
</head>
<style type="text/css">
    li.parsley-minlength,
    li.parsley-type {
        color: red;
    }
</style>

